{
  "name": "ux-capture",
  "version": "2.0.1",
  "description": "Browser instrumentation helper that makes it easier to capture UX speed metrics",
  "main": "js/ux-capture.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetup/ux-capture.git"
  },
  "keywords": [
    "webperf",
    "perfmatters",
    "web",
    "performance",
    "ux",
    "speed",
    "user-timing"
  ],
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "build:clean": "rm js/ux-capture.min.js",
    "test": "jest test/*.test.js",
    "test:watch": "jest test/*.test.js --watch --verbose --coverage",
    "test:coverage": "jest test/*.test.js --coverage"
  },
  "author": "Sergey Chernyshev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meetup/ux-capture/issues"
  },
  "homepage": "https://github.com/meetup/ux-capture#readme",
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.6.0",
    "uglify-es": "^3.3.9",
    "usertiming": "^0.1.8",
    "webpack": "^4.18.1",
    "webpack-cli": "^3.1.0"
  }
